source <(docker completion bash)

_docker_compose_exec_local_file_completion() {
  if [[ "${COMP_CWORD}" -ge 4 && "${COMP_WORDS[1]}" == 'compose' && "${COMP_WORDS[2]}" == 'exec' ]]; then
    compopt -o default
  else
    __start_docker
  fi
}

complete -F _docker_compose_exec_local_file_completion docker
